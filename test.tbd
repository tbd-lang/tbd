type list('a) =
    | Empty
    | Node('a, list('a))

fun rec iter(f, l) {
    match l {
        Empty -> (),
        Node(hd, tl) -> {
            f(hd);
            iter(f, tl)
        }
    }
}

fun main() {
    let l = Node("I", Node("love", Node("you!", Empty)));
    iter(fun(s) { print_endline(s) }, l)
}